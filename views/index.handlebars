<html>
  <head><script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script></head>
  <body>
   <h2>Welcome Azure Application Setting</h2>
<h3>Access token :   </h3> {{#if data.accesstoken}} Received {{/if}}

<label><h3>All are madatory</h3></label>
<form method="post" action="setvalue">
<table>
  <tr>
    <td><label>Subscription Id</label></td>
    <td><input type="text" name="subscriptionid"  id="subscriptionid" value="{{data.subscriptionId}}" required></td>
</tr>
<tr>
 <td><label>Resource Group</label></td>
    <td><input type="text" name="rg" id="rg" value="{{data.rg}}" required></td>

</tr>
    <tr>
<td><label>WebApp Name</label></td>
    <td><input type="text" name="appname" id="appname" value="{{data.appname}}" required></td>
 
    </tr>
    <tr>
<td><label>Tenant Id</label></td>
    <td><input type="text" name="tenantid" id="tenantid" value="{{data.tenantid}}" required></td>
 
    </tr>
    <tr>
 <td><label>Appid/Client Id</label></td>
    <td><input type="text" name="client_id" id="client_id" value="{{data.client_id}}" required></td>

    </tr>
    <tr>
<td></td>
    <td>  <input type="submit" value="Set Value">
</td>

    </tr>

  </tr>
</table>
</form>

<form method="POST" action="update">

  <label>Existing Application Setting Value</label>
  <br>
  <table class="rs">
  {{#each keyval}}
<tr class="rs">
  <td class="rs">{{this.key}}
</td>
 <td class="rs">{{this.value}}
</td>
</tr>
  {{/each}}
  </table >
  <label>{{exval}}</label>
  <br><br>
  <textarea rows="10" cols="50" name="keyjson" value="{{exval}}" required></textarea>
<input type="submit" value="Set Key Value Json">
</form>
</body>
<script>
 
</script>
<style>
  .rs {
  border: 1px solid black;
}
</style>
</html>

